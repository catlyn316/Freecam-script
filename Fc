local clickTpEnabled = false
local cameraCFrame = camera.CFrame
local moveSpeed = 1.2
local verticalSpeed = 0.5
local rotationSpeed = 0.015

-- è§¸æ§æ§åˆ¶è®Šé‡
local touchControls = {}
local moveTouch = nil
local lookTouch = nil
local speedTouch = nil

-- å‰µå»ºè§¸æ§æ§åˆ¶ GUI
local function createTouchGUI()
